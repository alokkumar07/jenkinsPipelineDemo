pipeline {
    agent any

    stages {
        
       stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Build') {
            steps {
                echo 'Building'
            }
        }
        stage('deploy') {
            steps {
                echo 'deploying'
            }
        }
        stage('test') {
            steps {
                echo 'Testing'
            }
        }
        stage('release') {
            steps {
                echo 'Releasing'
            }
        }
        
    }
}
